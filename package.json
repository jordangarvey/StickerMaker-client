		"@babel/runtime": "^7.4.5",
		"@types/jest": "^24.0.15",
		"@types/react": "^16.8.22",
		"@types/react-native": "^0.57.63",
		"@types/react-native-elements": "^0.18.0",
		"@types/react-navigation": "^3.0.7",
