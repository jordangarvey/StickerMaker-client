		"typescript": "^3.5.2"
	},
	"jest": {
		"preset": "react-native",
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
			"^.+\\.tsx?$": "ts-jest"
		},
		"globals": {
			"ts-jest": {
				"tsConfig": "tsconfig.jest.json"
			}
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"modulePaths": [
			"<rootDir>"
		]
	}
